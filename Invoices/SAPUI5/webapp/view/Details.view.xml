<mvc:View
    controllerName="logaligroup.SAPUI5.controller.Details"
    xmlns:m="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:logali="logaligroup.SAPUI5.control"
    displayBlock="true">
    <m:Page title="{i18n>detailsPageTitle}" showNavButton="true" navButtonPress=".onNavBack">
        <m:ObjectHeader title="{northwind>Quantity} x {northwind>ProductName}">
            <m:attributes>
                <m:ObjectAttribute text="{northwind>ShipName}"/>
                <m:ObjectAttribute text="{northwind>ShipAddress}"/>
                <m:ObjectAttribute text="{northwind>ShipCity}"/>
                <m:ObjectAttribute text="{northwind>ShipRegion}"/>
                <m:ObjectAttribute text="{northwind>ShipPostalCode}"/>
                <m:ObjectAttribute text="{northwind>ShipCountry}"/>
                <m:ObjectAttribute text="{northwind>CustomerID}"/>
                <m:ObjectAttribute text="{northwind>CustomerName}"/>
            </m:attributes>
        </m:ObjectHeader>
        <logali:ProductRating id="rating" class="sapUiSmallMarginBeginEnd" change=".onRatingChange"/> 
    </m:Page>
</mvc:View>